class TestClass 
{
  static void main(def args)
  {
    println "Hi Mehar it's me"
  }
}


pipeline {
  agent any
  options {
        timestamps()
    }
  stages{
    stage('Shared Library') {
              steps {
               script {
	       groovy TestClass.groovy
              }
   	     }
          }
     
       }

} 
